<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="com.hobba_HOBAServer_war_1.0-SNAPSHOTPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.hobba.hobaserver.services.User</class>
    <class>com.hobba.hobaserver.services.Chalenges</class>
    <class>com.hobba.hobaserver.services.Devices</class>
    <class>com.hobba.hobaserver.services.Keys</class>
    <class>com.hobba.hobaserver.entitymanager.HobaDevices</class>
    <class>com.hobba.hobaserver.entitymanager.HobaChalenges</class>
    <class>com.hobba.hobaserver.entitymanager.HobaKeys</class>
    <class>com.hobba.hobaserver.entitymanager.HobaUser</class>
    <class>com.hobba.hobaserver.services.HobaChalenges</class>
    <class>com.hobba.hobaserver.services.HobaDevices</class>
    <class>com.hobba.hobaserver.services.HobaUser</class>
    <class>com.hobba.hobaserver.services.HobaKeys</class>
    <class>com.hobba.hobaserver.services.HobaToken</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://127.0.0.1:5432/hoba"/>
